        </div>
      </div>

      <!-- content -->
      <div class="container-fluid">

        <style>
          .error-page-container{
            background: #fff;
            position: relative;
            border-radius: 1rem;
           box-shadow: 0 8px 18px -2px rgba(0, 0, 0, 0.2);
           overflow: hidden;
          }
          
          .error-page-container .left-side{
             height: 70vh;
           display: flex;
          }
          
          .error-page-container figure{
            display: flex;
            justify-content: center;
            align-items: center;
            width: 100%;
            
            img{
              max-width: 100%;
              z-index: 0;
            }
          }
          .error-page-container figure::before{
            position: absolute;
            content: '';
            height: 200px;
            width: 200px;
            background-color: #e7f6fd;
            border-radius: 50%;
          
          }
          
          
          .error-page-container .right-side{
            display: flex;
            flex-direction: column;
            justify-content: center;
            height: 70vh;
            padding-inline: 2rem;
          
            h1{
              font-weight: 800;
              font-size: 3rem;
              color: #2FC4F3;
            }
          
            p{
              font-weight: 400;
               font-size: 1.5rem;
               font-weight: 600;
               margin-block: 1rem;
               line-height: 2rem;
             
            }
          }
          
          
          .error-page-container .dot span{
            position: relative;
          }
          .span-container{
            display: flex;
            gap: 25px;
            margin-block: 25px;
          }
          .error-page-container .dot span::after{
            content: '';
            position: absolute;
            height: 13px;
            width: 13px;
            background-color: #c5d9ef;
            border-radius: 50%;
          }
          
          .dot{
            position: absolute;
            right: 30px;
            bottom:-18px ;
          }
          
          .error-page-container .left-dot span{
           
            position: relative;
          }
          
          .error-page-container .left-dot span::after{
            content: '';
            position: absolute;
            height: 13px;
            width: 13px;
            background-color: #c5d9ef;
            border-radius: 50%;
          
          }
          
          .left-dot{
            position: absolute;
            left: 18px;
            top:-29px ;
          }
            .cntc-btn{
              width: 15rem;
              padding: 10px;
              border: none;
              background-color:#2FC4F3;
              color:white;
              font-weight: 600;
              border-radius: 5px;
              margin-top: 1rem;
              outline: none;
            }
          </style>
          
              <!-- charts -->    
            <div class="chart">
                <div class="row">
                    <div class="error-page-container my-5 container">
                      <div class="left-dot">
                             <div class="span-container">
                               <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                             </div>
                              <div  class="span-container">
                               <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                             </div>
                              <div  class="span-container">
                               <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                             </div>
                      </div>
                      <div class="row">
                          <div class="col-lg-6 col-12 left-side  ">
                              <figure>
                                  <img src="../images/error-element.jpeg" alt="Error" >
                              </figure>
                          </div>
                          <div class="col-lg-6 col-12 right-side">
                              <h1>Technical Error</h1>
                              <p>Please wait for some time or contact Lifelinkr support team</p>
                              <a class="cntc-btn text-center" href="mail:support@lifelinkr.com">Contact LifeLinkr</a>
                          </div>
                      </div>
                       <div class="dot">
                             <div class="span-container">
                               <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                             </div>
                              <div class="span-container">
                               <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                             </div>
                              <div class="span-container">
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                              <span></span>
                             </div>
                      </div>
          
                       
                  </div>
          
                 </div>
          </div>

      </div>

      <%- include('./footer') %>
